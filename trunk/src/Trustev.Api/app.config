<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IAuthenticationService">
                    <security mode="Transport" />
                </binding>
                <binding name="BasicHttpBinding_IProfileService">
                    <security mode="Transport" />
                </binding>
                <binding name="BasicHttpBinding_ISocialService">
                    <security mode="Transport" />
                </binding>
                <binding name="BasicHttpBinding_ITransactionService">
                    <security mode="Transport" />
                </binding>
                <binding name="BasicHttpBinding_IIdentityService">
                    <security mode="Transport" />
                </binding>
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="https://127.0.0.1:444/v1.1/AuthenticationService.svc/soap"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IAuthenticationService"
                contract="v1_1.Services.Authentication.IAuthenticationService"
                name="BasicHttpBinding_IAuthenticationService" />
            <endpoint address="https://127.0.0.1:444/v1.1/ProfileService.svc/soap"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IProfileService"
                contract="v1_1.Services.Profile.IProfileService" name="BasicHttpBinding_IProfileService" />
            <endpoint address="https://127.0.0.1:444/v1.1/SocialService.svc/soap"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ISocialService"
                contract="v1_1.Services.Social.ISocialService" name="BasicHttpBinding_ISocialService" />
            <endpoint address="https://127.0.0.1:444/v1.1/TransactionService.svc/soap"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ITransactionService"
                contract="v1_1.Services.Transaction.ITransactionService" name="BasicHttpBinding_ITransactionService" />
            <endpoint address="https://127.0.0.1:444/v1.1/IdentityService.svc/soap"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IIdentityService"
                contract="v1_1.Services.Identity.IIdentityService" name="BasicHttpBinding_IIdentityService" />
        </client>
    </system.serviceModel>
</configuration>